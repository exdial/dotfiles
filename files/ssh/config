Host *
  User                  ubuntu
  IdentityFile          ~/.ssh/id_ed25519
  AddKeysToAgent        yes
  ForwardX11            no
  ForwardAgent          no
  LogLevel              ERROR
  ControlMaster         auto
  ControlPersist        120
  ControlPath           ~/.ssh/tmp/%h_%p_%r
  TCPKeepAlive          yes
  UserKnownHostsFile    /dev/null
  StrictHostKeyChecking false
